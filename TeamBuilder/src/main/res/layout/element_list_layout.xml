            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_ice"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_fighting"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_psychic"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_ghost"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_dark"
                android:layout_width="match_parent"
