<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/search_panel_element_list_background"
    android:layout_gravity="center_horizontal">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_normal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_fire"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_water"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_electric"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_grass"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_flying"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_bug"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_rock"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_ground"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_poison"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_steel"
