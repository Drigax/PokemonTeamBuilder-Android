            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_dragon"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.drigax.teambuilder.view_code_behind.controls.ElementSlotView
                android:id="@+id/search_panel_element_list_fairy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </LinearLayout>

    </ScrollView>
